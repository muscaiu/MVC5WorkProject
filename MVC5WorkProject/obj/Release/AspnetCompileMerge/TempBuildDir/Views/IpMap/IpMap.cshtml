    <script>
        //Function that shows the Fade Editor on Click
        $(function () {
            var btnid = $(this).attr("Id");
            var showEditor = function () {
                $("#btnEditor").modal("show");
            };

            $(".btn-default").click(showEditor);
            $(".btn-default").click(function () {
                $("#labelValue").html(btnid);
            });

            $("#btnSave").on("click", function () {
                $("#labelValue").html(btnid);
            });
        });
    </script>




